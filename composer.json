{
	"name": "orchestra/resources",
	"description": "Orchestra Platform Resources Component",
	"homepage": "http://orchestraplatform.com/docs/2.0/components/resources/",
	"keywords": ["orchestra-platform", "orchestral", "laravel"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-0": { 
			"Orchestra\\Resources" : "src/" 
		}
	},
	"require": {
		"php": ">=5.3.3",
		"orchestra/facile": "self.version"
	},
	"require-dev": {
		"mockery/mockery": "0.8.0",
		"illuminate/http": "4.0.*",
		"illuminate/routing": "4.0.*",
		"satooshi/php-coveralls": "~0.6"
	},
	"suggest": {
		"laravel/framework": "Allow using orchestra/resources component outside of Orchestra Platform",
		"orchestra/foundation": "Allow using orchestra/resources with Orchestra Platform"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.1-dev"
		}
	},
	"minimum-stability": "dev"
}
