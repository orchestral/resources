{
	"name": "orchestra/resources",
	"description": "Resources Component for Orchestra Platform",
	"homepage": "http://orchestraplatform.com/docs/latest/components/resources/",
	"keywords": ["orchestra-platform", "orchestral", "laravel"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-4": {
			"Orchestra\\Resources\\" : "src/Resources/"
		}
	},
	"require": {
		"php": ">=5.4.0",
		"orchestra/routing": "3.0.*"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"illuminate/events": "5.0.*",
		"illuminate/http": "5.0.*",
		"orchestra/facile": "3.0.*"
	},
	"suggest": {
		"laravel/framework": "Allow using orchestra/resources component outside of Orchestra Platform",
		"orchestra/foundation": "Allow using orchestra/resources with Orchestra Platform"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		}
	},
	"minimum-stability": "dev"
}
