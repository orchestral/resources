{
	"name": "orchestra/resources",
	"description": "Orchestra Platform 2 Resources Component",
	"homepage": "http://orchestraplatform.com/docs/latest/components/resources/",
	"keywords": ["orchestra-platform", "orchestral", "laravel"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-0": {
			"Orchestra\\Resources" : "src/"
		}
	},
	"require": {
		"php": ">=5.3.3",
		"orchestra/facile": "2.1.*"
	},
	"require-dev": {
		"mockery/mockery": "0.8.*",
		"illuminate/events": "4.1.*",
		"illuminate/http": "4.1.*",
		"illuminate/routing": "4.1.*"
	},
	"suggest": {
		"laravel/framework": "Allow using orchestra/resources component outside of Orchestra Platform 2",
		"orchestra/foundation": "Allow using orchestra/resources with Orchestra Platform 2"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.1-dev"
		}
	},
	"minimum-stability": "dev"
}
